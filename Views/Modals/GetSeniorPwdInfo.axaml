<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="600"
        x:Class="EBISX_POS.Views.GetSeniorPwdInfo"
        Title="Get Senior Pwd Info"
        WindowStartupLocation="CenterScreen"
        ExtendClientAreaToDecorationsHint="True"
        SystemDecorations="None"
        Width="500" Height="650"
        CornerRadius="10">
	<Border BorderThickness="15" CornerRadius="10" BorderBrush="Orange">
		<ScrollViewer Margin="10">
			<StackPanel Orientation="Vertical" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Center">
				<StackPanel x:Name="InputStackPanel" Spacing="10"/>
				<Button Content="Submit"
						Width="130" Height="40"
						FontWeight="SemiBold"
						FontFamily="{StaticResource NunitoFont}"
						FontSize="16"
						Margin="20,0,0,0"
						VerticalContentAlignment="Center"
						HorizontalContentAlignment="Center"
						Click="SubmitButton_Click"/>
			</StackPanel>
		</ScrollViewer>
	</Border>
</Window>
